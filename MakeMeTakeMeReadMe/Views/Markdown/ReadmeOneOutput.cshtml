      <p>@Model.SectionHeaders("bugOpen")@Model.SectionHeaders("bugStatus")@Model.SectionHeaders("bugMiddle")@Model.SectionHeaders("bugDate")@Model.SectionHeaders("bugClose")</p>
      <br>
      <p>@Model.SectionHeaders("contributorsSectionHeader")</p>
      <br>
      <p>@Model.SectionHeaders("contributorStartMarkdown")</p>
