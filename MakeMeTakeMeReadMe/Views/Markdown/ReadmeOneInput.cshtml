            <br>
            <span>{Asp.Net MVC: <input type="checkbox" name="usesAspDotnetMvc" value="true">}
            <br>
            <span>{HTML: <input type="checkbox" name="usesHtml" value="true">}
            <br>
            <span>{NodeJs: <input type="checkbox" name="usesNodeJs" value="true">}
            <br>
            <span>{Webpack: <input type="checkbox" name="usesWebpack" value="true">}
            <br>
            <span>{Jquery: <input type="checkbox" name="usesJquery" value="true">}
          </div>
        </div>

        @* keywords *@
        <div class="col-md-6">
          <div class="form-group">
            <label for="keyword1">keyword 1</label>
